#include <stdio.h>
#include <stdlib.h>

typedef struct Node {
    char* data;
    struct Node* next;
    struct Node* prev;
} Node;

int main() {
    Node* node1 = (Node*) malloc(sizeof(Node));
    node1->data = "Bogie 1";
    node1->next = NULL;
    node1->prev = NULL;

    Node* node2 = (Node*) malloc(sizeof(Node));
    node2->data = "Bogie 2";
    node2->next = NULL;
    node2->prev = node1;
    node1->next = node2;

    Node* node3 = (Node*) malloc(sizeof(Node));
    node3->data = "Bogie 3";
    node3->next = NULL;
    node3->prev = node2;
    node2->next = node3;

    Node* node4 = (Node*) malloc(sizeof(Node));
    node4->data = "Bogie 4";
    node4->next = NULL;
    node4->prev = node3;
    node3->next = node4;

    Node* node5 = (Node*) malloc(sizeof(Node));
    node5->data = "Bogie 5";
    node5->next = NULL;
    node5->prev = node4;
    node4->next = node5;

    Node* node6 = (Node*) malloc(sizeof(Node));
    node6->data = "Bogie 6";
    node6->next = NULL;
    node6->prev = node5;
    node5->next = node6;

    Node* currentNode = node1;
    printf("Forward: ");
    while (currentNode) {
        printf("%s -> ", currentNode->data);
        currentNode = currentNode->next;
    }
    printf("NULL\n");

    currentNode = node6;
    printf("Backward: ");
    while (currentNode) {
        printf("%s -> ", currentNode->data);
        currentNode = currentNode->prev;
    }
    printf("NULL\n");

    free(node1);
    free(node2);
    free(node3);
    free(node4);

    return 0;
}

//C
