public class Main {

    static class Node {
        String data;
        Node next;

        Node(String data) {
            this.data = data;
            this.next = null;
        }
    }

    public static void main(String[] args) {
        Node node1 = new Node("Bogie 1");
        Node node2 = new Node("Bogie 2");
        Node node3 = new Node("Bogie 3");
        Node node4 = new Node("Bogie 4");
        Node node5 = new Node("Bogie 5");

        node1.next = node2;
        node2.next = node3;
        node3.next = node4;
        node4.next = node5;
        node5.next = node1;  // Circular link

        Node currentNode = node1;
        Node startNode = node1;
        System.out.print(currentNode.data + " -> ");
        currentNode = currentNode.next;

        while (currentNode != startNode) {
            System.out.print(currentNode.data + " -> ");
            currentNode = currentNode.next;
        }

        System.out.println("Bogie Next");  // Indicating the list loops back
    }
}

//Java
