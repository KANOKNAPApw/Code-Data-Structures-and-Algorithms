class Node:
    def __init__(self, data):
        self.data = data
        self.next = None
    
node1 = Node("Bogie 1")
node2 = Node("Bogie 2")
node3 = Node("Bogie 3")
node4 = Node("Bogie 4")
node5 = Node("Bogie 5")

node1.next = node2
node2.next = node3
node3.next = node4
node4.next = node5 
node5.next = node1 # Circular link

currentNode = node1
startNode = node1
print(currentNode.data, end=" -> ") 
currentNode = currentNode.next 

while currentNode != startNode:
    print(currentNode.data, end=" -> ")
    currentNode = currentNode.next

print("Bogie Next") # Indicating the list loops back

#Python
