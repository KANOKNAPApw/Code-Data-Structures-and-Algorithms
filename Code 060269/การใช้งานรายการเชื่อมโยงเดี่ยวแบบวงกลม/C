#include <stdio.h>
#include <stdlib.h>

typedef struct Node {
    char* data;
    struct Node* next;
} Node;

int main() {
    Node* node1 = (Node*) malloc(sizeof(Node));
    Node* node2 = (Node*) malloc(sizeof(Node));
    Node* node3 = (Node*) malloc(sizeof(Node));
    Node* node4 = (Node*) malloc(sizeof(Node));
    Node* node5 = (Node*) malloc(sizeof(Node));

    node1->data = "Bogie 1";
    node2->data = "Bogie 2";
    node3->data = "Bogie 3";
    node4->data = "Bogie 4";
    node5->data = "Bogie 5";

    node1->next = node2;
    node2->next = node3;
    node3->next = node4;
    node4->next = node5;
    node5->next = node1;  // Circular link

    Node* currentNode = node1;
    Node* startNode = node1;
    printf("%s -> ", currentNode->data);
    currentNode = currentNode->next;

    while (currentNode != startNode) {
        printf("%s -> ", currentNode->data);
        currentNode = currentNode->next;
    }

    printf("Bogie Next\n");  // Indicating the list loops back

    free(node1);
    free(node2);
    free(node3);
    free(node4);
    free(node5);

    return 0;
}

//C
